<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.2" />
  </startup>
  <appSettings>
    <add key="login" value="https://login.windows.net/{0}" />
    <add key="tenantId" value="id-of-tenent" />
    <add key="apiEndpoint" value="https://management.core.windows.net/" />
    <add key="appId" value="id-of-app" />
    <add key="redirectUri" value="uri-of-application" />
    <add key="subscriptionId" value="id-of-subscription" />
    <add key="groupName" value="name-of-the-resource-group" />
    <add key="storageName" value="name-of-storage" />
    <add key="ipName" value="name-of-ip" />
    <add key="vnetName" value="name-of-vnet" />
    <add key="subnetName" value="name-of-subnet" />
    <add key="nicName" value="name-of-nic" />
    <add key="availabilityName" value="name-of-availability-set" />
    <add key="vmName" value="name-of-virtual-machine" />
    <add key="adminName" value="name-of-admin-account" />
    <add key="adminPassword" value="password-of-account" />
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="9.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>